<?xml version="1.0" ?>
<robot name="pppbot">
  <link name="base_link"/>
  <link name="obstacle1">
    <inertial>
      <origin rpy="0 0 0" xyz=".05 .05 .05"/>
      <mass value="1.0"/>
      <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
