  <geometry>
    <box size="0.0254 0.3048 0.762"/>
  </geometry>
  <origin xyz="0.0127 -0.1524 -0.381"/>
</collision>
<collision name="cabinet_${id}_sideR">
<geometry>
  <box size="0.0254 0.3048 0.762"/>
</geometry>
<origin xyz="0.5969 -0.1524 -0.381"/>
</collision>
<collision name="cabinet_${id}_cabinet1">
     <geometry>
